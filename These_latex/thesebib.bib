	langid = {english},
	file = {Bedford et al. - 2022 - Fault rock heterogeneity can produce fault weaknes.pdf:/home/yohann/Zotero/storage/DZMRDL9H/Bedford et al. - 2022 - Fault rock heterogeneity can produce fault weaknes.pdf:application/pdf},
