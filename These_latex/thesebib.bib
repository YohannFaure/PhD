	number = {2},
	journaltitle = {Journal of Geophysical Research: Solid Earth},
	shortjournal = {{JGR} Solid Earth},
	author = {Aharonov, Einat and Scholz, Christopher H.},
	urldate = {2024-04-22},
	date = {2018-02},
	langid = {english},
	file = {A Physics‐Based Rock Friction Constitutive Law\: Steady State Friction:/home/yohann/Zotero/storage/EFQE2KJE/aharonov2017.pdf.pdf:application/pdf;Full Text:/home/yohann/Zotero/storage/63LWA7IJ/Aharonov and Scholz - 2018 - A Physics‐Based Rock Friction Constitutive Law St.pdf:application/pdf},
